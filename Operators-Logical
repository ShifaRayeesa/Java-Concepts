public class Logical {
    public static void main(String[] args)
    {
        System.out.println("Logical AND");
        System.out.println(true && true);
        System.out.println(true && false);
        System.out.println();
        System.out.println("Logical OR");
        System.out.println(true || true);
        System.out.println(true || false);
    }
}
